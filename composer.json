{
    "name": "wcr/entitize",
    "description": "A crud package for Laravel, with bootstrap and former",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco Monti",
            "email": "f.monti@webcrafters.it",
            "homepage": "http://www.webcrafters.it"
        }
    ],
    "homepage": "https://github.com/francescomonti/entitize",
    "keywords": ["Laravel", "Entitize"],
    "require": {
        "illuminate/support": "~5",
        "anahkiasen/former": "^4.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Wcr\\Entitize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wcr\\Entitize\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wcr\\Entitize\\EntitizeServiceProvider"
            ],
            "aliases": {
                "Entitize": "Wcr\\Entitize\\Facades\\Entitize"
            }
        }
    }
}
